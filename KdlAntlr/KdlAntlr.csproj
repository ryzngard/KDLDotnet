<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="antlr-4.9-complete.jar" />
    <None Remove="antlr.bat" />
    <None Remove="antlr_compile.bat" />
    <None Remove="grun.bat" />
    <None Remove="*.java" />
    <None Remove="*.class" />
    <None Remove="*.tokens" />
